define ["../../lib/lodash"], (_) ->
    map = "
        ................................
        .r..........................r...
        b...............................
        ................................
        ................................
        ................................
        ................................
        ................................
        ...............g................
        ................................
        ................................
        ................................
        ................................
        ................................
        ................................
        ...............P................
        ................................
        ................................
    ".split(' ').map (r) -> r.split ''

    key =
        '.' : 'empty'
        'P' : 'player'
        'r' : 'red'
        'b' : 'blue'
        'g' : 'green'

    for c in _.flatten(map) when c isnt ' '
        if not (c of key)
            console.warn "unknown character in map '#{c}'"

    return {
        map
        key
    }
